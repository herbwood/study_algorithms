    "# 2455\n",
    "def smart_train():\n",
    "    current = 0\n",
    "    blist = []\n",
    "    for i in range(4):\n",
    "        a, b = map(int, input().split())\n",
    "        current += b - a\n",
    "        blist.append(current)\n",
    "    return max(blist)\n",
    "\n",
    "print(smart_train())"
   ]
