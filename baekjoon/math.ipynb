    "# 1977\n",
    "m = int(input())\n",
    "n = int(input())\n",
    "mp, np = int(m ** 0.5), int(n ** 0.5)\n",
    "blist = []\n",
    "for i in range(mp, np + 1):\n",
    "    if i ** 2 in range(m, n + 1):\n",
    "        blist.append(i ** 2)\n",
    "if len(blist) == 0:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(sum(blist))\n",
    "    print(min(blist))"
   ]
